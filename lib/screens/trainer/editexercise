
void editExercise(String docId, String newTitle, String newDescription) async {
    await FirebaseFirestore.instance.collection('WeightlossWorkout').doc(docId).update({
      'title': newTitle,
      'description': newDescription,
    });
  }
  Stream<QuerySnapshot> fetchExercises() {
    return FirebaseFirestore.instance.collection('WeightlossWorkout').snapshots();
  }
